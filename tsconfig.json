{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "noImplicitAny": true,
    "module": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "target": "es2020",
    "types": [
      "@emotion/react/types/css-prop"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["dist", "node_modules"],
  "files": ["src/index.tsx"]
}
